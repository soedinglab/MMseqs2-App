{
  "name": "mmseqs-app",
  "productName": "MMseqs2 App",
  "version": "1.5.0",
  "author": "Milot Mirdita <milot@mirdita.de>",
  "description": "MMseqs2 is a software suite to search and annotate huge sequence sets. We built a graphical interface to make it more useful for interactive data exploration.",
  "repository": "https://github.com/soedinglab/MMseqs2-App",
  "license": "GPL-3.0-or-later",
  "main": "dist/main.js",
  "scripts": {
    "electron:deps": "make all",
    "electron:build": "npm run electron:deps && cross-env ELECTRON=1 node ./electron/build.js && electron-builder --x64 -mwl",
    "electron:dir": "npm run electron:deps && cross-env ELECTRON=1 node ./electron/build.js && electron-builder --x64 --dir",
    "electron:dev": "cross-env ELECTRON=1 node ./electron/dev-runner.js",
    "electron:pack": "npm run electron:main && cross-env ELECTRON=1 npm run frontend",
    "electron:main": "webpack -p --progress --hide-modules --colors --config ./webpack.electron.config.js",
    "frontend": "webpack -p --progress --hide-modules  --colors --config ./webpack.frontend.config.js",
    "frontend:watch": "webpack --mode development --watch --progress --hide-modules --colors --config ./webpack.frontend.config.js",
    "frontend:dev": "webpack-dev-server --mode development --open --inline --hot --content-base dist/ --config ./webpack.frontend.config.js",
    "frontend:webpack-profile": "webpack -p --profile --json --config ./webpack.frontend.config.js > dist/webpack-stats.json",
    "result": "webpack -p --progress --hide-modules  --colors --config ./webpack.result.config.js",
    "result:watch": "webpack --mode development --watch --progress --hide-modules --colors --config ./webpack.result.config.js",
    "result:dev": "webpack-dev-server --mode development --open --inline --hot --content-base dist/ --config ./webpack.result.config.js",
    "api": "raml2html --template docs/api/theme/index.nunjucks docs/api/api.raml > docs/api.html"
  },
  "configuration": {
    "apiEndpoint": "https://search.mmseqs.com"
  },
  "build": {
    "productName": "MMseqs2 Search",
    "appId": "com.mmseqs.app",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/**/*"
    ],
    "extraFiles": [
      {
        "from": "resources/${os}",
        "to": "resources/bin"
      }
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "resources/icons/icon.icns"
    },
    "win": {
      "icon": "resources/icons/icon.ico"
    },
    "linux": {
      "icon": "resources/icons/icon.icns",
      "category": "Science"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.5",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/runtime": "^7.9.2",
    "app-root-dir": "^1.0.2",
    "babel-loader": "^8.1.0",
    "compression-webpack-plugin": "^3.1.0",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.2",
    "del": "^5.1.0",
    "devtron": "^1.4.0",
    "electron": "^8.3.2",
    "electron-builder": "^22.4.1",
    "electron-debug": "^3.1.0",
    "electron-devtools-installer": "^2.2.4",
    "favicons-webpack-plugin": "^2.1.0",
    "file-loader": "^4.3.0",
    "find-free-port": "=1.2.0",
    "html-webpack-plugin": "^3.2.0",
    "icon-gen": "^2.0.0",
    "material-design-icons": "^3.0.1",
    "mini-css-extract-plugin": "^0.9.0",
    "nunjucks": "^3.2.1",
    "raml2html": "^7.5.0",
    "style-loader": "^1.2.1",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2",
    "svg2png": "^4.1.1",
    "url-loader": "^4.1.0",
    "vue": "^2.6.11",
    "vue-loader": "^15.9.1",
    "vue-localstorage": "^0.6.2",
    "vue-resource": "^1.5.1",
    "vue-router": "^3.1.6",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.11",
    "vuedraggable": "^2.23.2",
    "vuetify": "^1.5.24",
    "vuetify-loader": "^1.5.0",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-subresource-integrity": "^1.5.1"
  }
}
